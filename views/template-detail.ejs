<script>
  document.addEventListener("DOMContentLoaded", () => {
    const templateMainImage = document.getElementById("template-main-image");
    const zoomInBtn = document.querySelector(".zoom-in");
    const zoomOutBtn = document.querySelector(".zoom-out");
    const zoomPercDisplay = document.querySelector(".zoom-perc");

    let currentZoom = 1; // Initial zoom level (1x or 100%)
    const zoomStep = 0.1; // Increment for zooming
    const minZoom = 1; // Minimum zoom level (100%)
    const maxZoom = 3; // Maximum zoom level (300%)

    let isDragging = false;
    let startX, startY, initialX, initialY;

    // Update zoom level and apply scale
    const updateZoom = () => {
      templateMainImage.style.transform = `scale(${currentZoom})`;
      zoomPercDisplay.textContent = `${Math.round(currentZoom * 100)}%`;

      // Enable or disable buttons based on zoom level
      zoomInBtn.style.pointerEvents = currentZoom >= maxZoom ? "none" : "auto";
      zoomInBtn.style.opacity = currentZoom >= maxZoom ? 0.5 : 1;

      zoomOutBtn.style.pointerEvents = currentZoom <= minZoom ? "none" : "auto";
      zoomOutBtn.style.opacity = currentZoom <= minZoom ? 0.5 : 1;

      // Enable or disable dragging based on zoom level
      if (currentZoom > 1) {
        templateMainImage.style.cursor = "grab";
      } else {
        templateMainImage.style.cursor = "default";
        templateMainImage.style.transform = "translate(0, 0) scale(1)";
      }
    };

    // Event listener for Zoom In
    zoomInBtn.addEventListener("click", () => {
      if (currentZoom < maxZoom) {
        currentZoom += zoomStep;
        updateZoom();
      }
    });

    // Event listener for Zoom Out
    zoomOutBtn.addEventListener("click", () => {
      if (currentZoom > minZoom) {
        currentZoom -= zoomStep;
        updateZoom();
      }
    });

    // Drag and move functionality
    templateMainImage.addEventListener("mousedown", (e) => {
      if (currentZoom > 1) {
        isDragging = true;
        startX = e.pageX;
        startY = e.pageY;
        initialX = parseFloat(
          templateMainImage.style.transform.match(/translate\((-?\d+\.?\d*px)/)?.[1] || "0"
        );
        initialY = parseFloat(
          templateMainImage.style.transform.match(/, (-?\d+\.?\d*px)/)?.[1] || "0"
        );
        templateMainImage.style.cursor = "grabbing";
      }
    });

    window.addEventListener("mousemove", (e) => {
      if (isDragging && currentZoom > 1) {
        const dx = e.pageX - startX;
        const dy = e.pageY - startY;
        let newX = (initialX || 0) + dx;
        let newY = (initialY || 0) + dy;

        // Check if the cursor has gone outside the image and reset position
        const imageRect = templateMainImage.getBoundingClientRect();
        const containerRect = templateMainImage.parentElement.getBoundingClientRect();

        // Prevent image from going outside of the container bounds
        if (imageRect.left + newX > containerRect.left) {
          newX = containerRect.left - imageRect.left;
          console.log(newX, "aaaaaaaaaaaaaaaaaaaa");

        }
        if (imageRect.top + newY > containerRect.top) {
          newY = containerRect.top - imageRect.top;
          console.log(newX, "aaaaaaaaaaaaaaaaaaaa");
        }
        if (imageRect.right + newX < containerRect.right - imageRect.width) {
          newX = containerRect.right - imageRect.width - imageRect.left;
        }
        if (imageRect.bottom + newY < containerRect.bottom - imageRect.height) {
          newY = containerRect.bottom - imageRect.height - imageRect.top;
        }

        templateMainImage.style.transform = `translate(${newX}px, ${newY}px) scale(${currentZoom})`;
      }
    });

    window.addEventListener("mouseup", () => {
      if (isDragging) {
        isDragging = false;
        templateMainImage.style.cursor = "grab";
      }
    });
    updateZoom();
  });

</script>
